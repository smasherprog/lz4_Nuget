<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Force-Enable-lz4-v120-windesktop-msvcstl-dyn-rt-dyn-x86)' == '' And ('$(PlatformToolset)' != 'v120' Or '$(ApplicationType)' != '' Or ('$(Platform)' != 'Win32' And '$(Platform)' != 'x86'))">
